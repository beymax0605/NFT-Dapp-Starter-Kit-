{
  "name": "hashlips_art_engine",
  "version": "1.1.1",
  "description": "HashLips Art Engine is a tool used to create multiple different instances of artworks based on provided layers.",
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "assets": [
      "layers/**/*",
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "scripts": {
    "build": "node index.js",
    "generate": "node index.js",
    "initialize": "node src/initializeMetadata.js",
    "upload": "node src/uploadImages.js",
    "upload:metadata": "node src/uploadMetadata.js",
    "upload:prereveal": "node src/uploadPrereveal.js",
    "update": "node src/updateMetadata.js"
  },
  "author": "JM & Daniel Eugene Botha (HashLips)",
  "license": "MIT",
  "dependencies": {
    "@pinata/sdk": "^1.1.25",
    "canvas": "^2.8.0",
    "dotenv": "^16.0.1",
    "gif-encoder-2": "^1.0.5",
    "sha1": "^1.1.1"
  }
}
